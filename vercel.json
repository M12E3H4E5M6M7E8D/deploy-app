{
    "builds": [{
        "src" : "project/wsgi.py",
        "user" : "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime" : "python3.12.4"
        }   
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "project/wsgi.py"
        }
    ]
}